{
  "name": "auto-js-vscodeext-fixed",
  "displayName": "Auto.js-VSCodeExt-Fixed",
  "description": "基于Auto.js-VSCode-Extension，优化了项目传输速度、支持直接在output面板查看执行记录、支持查看开发文档",
  "version": "0.3.6",
  "publisher": "710850609",
  "icon": "logo.png",
  "repository": {
    "type": "github",
    "url": "https://github.com/710850609/Auto.js-VSCode-Extension"
  },
  "engines": {
    "vscode": "^1.17.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.openDocument",
    "onCommand:extension.startServer",
    "onCommand:extension.stopServer",
    "onCommand:extension.run",
    "onCommand:extension.runOnDevice",
    "onCommand:extension.stop",
    "onCommand:extension.stopAll",
    "onCommand:extension.save",
    "onCommand:extension.saveToDevice",
    "onCommand:extension.newProject",
    "onCommand:extension.runProject",
    "onCommand:extension.saveProject"
  ],
  "main": "./out/extension",
  "contributes": {
    "breakpoints": [
      {
        "language": "javascript"
      }
    ],
		"menus": {
			"editor/title": [
				{
					"when": "resourceLangId == javascript",
					"command": "extension.openDocument",
					"group": "navigation"
				}
			]
		},
    "commands": [
      {
        "command": "extension.openDocument",
        "title": "打开文档(Open Document)",
        "icon": "logo.png",
				"category": "Auto.js"
      },
      {
        "command": "extension.startServer",
        "title": "开启服务(Start Server)",
				"category": "Auto.js"
      },
      {
        "command": "extension.stopServer",
        "title": "停止服务(Stop Server)",
				"category": "Auto.js"
      },
      {
        "command": "extension.run",
				"title": "运行脚本(Run)",
				"category": "Auto.js"
      },
      {
        "command": "extension.runOnDevice",
				"title": "在指定设备运行脚本(Run On Device)",
				"category": "Auto.js"
      },
      {
        "command": "extension.stop",
				"title": "停止当前脚本(Stop)",
				"category": "Auto.js"
      },
      {
        "command": "extension.stopAll",
				"title": "停止所有脚本(Stop All)",
				"category": "Auto.js"
      },
      {
        "command": "extension.rerun",
				"title": "重新运行(Rerun)",
				"category": "Auto.js"
      },
      {
        "command": "extension.save",
				"title": "保存到所有设备(Save)",
				"category": "Auto.js"
      },
      {
        "command": "extension.saveToDevice",
				"title": "保存到指定设备(Save On Device)",
				"category": "Auto.js"
      },
      {
        "command": "extension.newProject",
				"title": "新建项目(New Project)",
				"category": "Auto.js"
      },
      {
        "command": "extension.saveProject",
				"title": "保存项目到设备(Save Project)",
				"category": "Auto.js"
      },
      {
        "command": "extension.runProject",
				"title": "运行项目(Run Project)",
				"category": "Auto.js"
      }
    ],
    "keybindings": [
      {
        "command": "extension.openDocument",
        "key": "f4",
        "mac": "f4",
        "when": "editorTextFocus"
      },
      {
        "command": "extension.run",
        "key": "f5",
        "mac": "f5",
        "when": "editorTextFocus"
      },
      {
        "command": "extension.stop",
        "key": "shift+f5",
        "mac": "shift+f5",
        "when": "editorTextFocus"
      },
      {
        "command": "extension.runOnDevice",
        "key": "ctrl+f5",
        "mac": "cmd+f5",
        "when": "editorTextFocus"
      },
      {
        "command": "extension.rerun",
        "key": "ctrl+shift+f5",
        "mac": "cmd+shift+f5",
        "when": "editorTextFocus"
      },
      {
        "command": "extension.save",
        "key": "ctrl+shift+S",
        "mac": "cmd+shift+S",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test",
    "install": "npm install",
    "package": "vsce package",
    "publish": "vsce publish"
  },
  "devDependencies": {
    "typescript": "^2.5.3",
    "vscode": "^1.1.5",
    "@types/node": "^7.0.43",
    "@types/mocha": "^2.2.42"
  },
  "dependencies": {
    "@types/archiver": "^2.1.2",
    "@types/jszip": "^3.1.4",
    "@types/stream-buffers": "^3.0.2",
    "@types/walk": "^2.3.0",
    "@types/websocket": "^0.0.40",
    "archiver": "^3.0.0",
    "jszip": "^3.1.5",
    "stream-buffers": "^3.0.2",
    "walk": "^2.3.14",
    "websocket": "1.0.26"
  },
  "__npminstall_done": false
}
